{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {  
        "indent" : [1.4],
        "linebreak-style" : 0,
        "no-console": "off",
        "semi": ["error", "never"],
        "object-curly-newline": ["error", "always"]
    }
}
